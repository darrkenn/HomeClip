<div class="w-full h-full flex flex-row space-x-12 space-y-4 p-4">
{{range .folders}}
    <a class="flex flex-col size-26" href="/folders?id={{.Id}}">
        <img src="/static/svg/folders/folder_{{.Colour}}.svg" class="" alt="Folder">
        <hr class="-mt-2.5 border-2 border-slate-200">
        <h1 class="text-white font-bold ">{{.Name}}</h1>
    </a>
{{end}}
</div>